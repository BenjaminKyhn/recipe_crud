<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.stage.Stage?>

<Stage title="IdealFood" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Main">
    <scene>
        <Scene>
            <AnchorPane prefHeight="546.0" prefWidth="822.0">
            <children>
               <ImageView fitHeight="594.0" fitWidth="1056.0" layoutX="-206.0" opacity="0.6" pickOnBounds="true" preserveRatio="true">
                  <image>
                     <Image url="https://i.imgur.com/KfSdfwo.jpg" />
                  </image>
               </ImageView>
                <ImageView fitHeight="420.0" fitWidth="825.0" layoutX="13.0" layoutY="-35.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="https://i.imgur.com/KGyHPQg.png" />
                    </image>
                </ImageView>
               <Button fx:id="storeIngredient" layoutX="482.0" layoutY="508.0" mnemonicParsing="false" onAction="#popupStoreIngredient" prefHeight="63.0" prefWidth="238.0" text="STORE INGREDIENT">
                  <font>
                     <Font name="Calibri Bold" size="22.0" />
                  </font>
               </Button>
               <Button fx:id="findRecipeButton" alignment="CENTER" layoutX="128.0" layoutY="508.0" mnemonicParsing="false" onAction="#popupChooseIngredients" prefHeight="64.0" prefWidth="238.0" text="FIND RECIPE" textAlignment="CENTER">
                  <font>
                     <Font name="Calibri Bold" size="22.0" />
                  </font>
               </Button>
            </children></AnchorPane>
        </Scene>
    </scene>
</Stage>
